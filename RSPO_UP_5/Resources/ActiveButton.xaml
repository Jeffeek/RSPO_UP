<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="ChoosedButton1" TargetType="Button">
        <Style.Triggers>
            <DataTrigger Binding="{Binding EquationNumber}" Value="1">
                <Setter Property="Background" Value="Aquamarine" />
            </DataTrigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="ChoosedButton2" TargetType="Button">
        <Style.Triggers>
            <DataTrigger Binding="{Binding EquationNumber}" Value="2">
                <Setter Property="Background" Value="Aquamarine" />
