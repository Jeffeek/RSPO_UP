<Window x:Class="RSPO_UP_3.FinishTestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RSPO_UP_3"
        mc:Ignorable="d"
        Title="FinishTestWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.1*"/>
            <RowDefinition Height="0.8*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="txtResult" Grid.Column="0" Grid.Row="1" Text="Вы набрали: {0} баллов!" Grid.ColumnSpan="3" FontSize="55" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Black"/>
        <Image x:Name="ImageResult" Grid.Column="1" Grid.Row="2" Source="Images/ResultImages/normy.png"/>
    </Grid>
</Window>
